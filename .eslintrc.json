{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": [
            "error",
            "always"
        ],
        "indent": [
            "error",
            2
        ],
        "no-unused-vars": "warn",
        "no-console": "warn"
    },
    "globals": {
        "$": "readonly",
        "jQuery": "readonly"
    }
}
